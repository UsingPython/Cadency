{
  "name": "discord-bot",
  "version": "0.0.0-development",
  "description": "A simple Discord-Music-Bot, written with discord.js and TypeScript",
  "homepage": "https://github.com/UsingPython/DiscordBot",
  "bugs": "https://github.com/UsingPython/DiscordBot/issues",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "yarn db:generate",
    "build": "tsc",
    "lint": "prettier . --check",
    "format": "prettier . --write",
    "test": "jest",
    "prestart": "yarn db:generate && yarn db:migrate:prod",
    "start": "node dist/main.js",
    "serve": "ts-node src/main.ts",
    "semantic-release": "semantic-release",
    "db:gui": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy"
  },
  "author": "UsingPython",
  "repository": {
    "type": "git",
    "url": "https://github.com/UsingPython/DiscordBot"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.5.3",
    "@prisma/client": "^2.26.0",
    "axios": "^0.21.1",
    "discord.js": "^12.5.3",
    "dotenv": "^10.0.0",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "query-string": "^7.0.0",
    "winston": "^3.3.3",
    "ytdl-core": "^4.8.0",
    "ytsr": "^3.5.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.5",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "axios-mock-adapter": "^1.19.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.0.6",
    "prettier": "^2.3.1",
    "prisma": "^2.25.0",
    "semantic-release": "^17.4.4",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.4"
  }
}
