{
  "name": "discord-bot",
  "version": "0.0.0-development",
  "description": "A simple Discord-Music-Bot, written with discord.js and TypeScript",
  "homepage": "https://github.com/jontze/Cadency",
  "bugs": "https://github.com/jontze/Cadency/issues",
  "main": "dist/main.js",
  "scripts": {
    "prebuild": "yarn db:generate",
    "build": "tsc",
    "lint": "prettier . --check",
    "format": "prettier . --write",
    "test": "jest",
    "prestart": "yarn db:generate && yarn db:migrate:prod",
    "start": "node dist/main.js",
    "serve": "ts-node src/main.ts",
    "semantic-release": "semantic-release",
    "db:gui": "prisma studio",
    "db:generate": "prisma generate",
    "db:migrate:reset": "prisma migrate reset",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:dev": "prisma migrate dev",
    "db:migrate:prod": "prisma migrate deploy"
  },
  "author": "jontze",
  "repository": {
    "type": "git",
    "url": "https://github.com/jontze/Cadency"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.9.0",
    "@prisma/client": "^4.11.0",
    "axios": "^0.27.2",
    "discord.js": "^12.5.3",
    "dotenv": "^16.0.3",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "query-string": "^7.1.3",
    "winston": "^3.8.2",
    "ytdl-core": "^4.11.4",
    "ytsr": "^3.8.0"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^27.5.0",
    "@types/node": "^18.15.10",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "axios-mock-adapter": "^1.21.2",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^27.5.1",
    "prettier": "^2.8.7",
    "prisma": "^4.11.0",
    "semantic-release": "^19.0.5",
    "ts-jest": "^27.1.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
